<sqlMap namespace="Registros">
    <insert id="setInscripcionEstudiante">
        insert into inscripciones (id_gestion,id_estudiante,id_curso,fecha_registro,id_gondola,id_familia,nombre_foto)
        values(#id_gestion#,#id_estudiante#,#id_curso#,now(),#id_gondola#,#id_familia#,#nombre_foto#);
    </insert>
</sqlMap>
